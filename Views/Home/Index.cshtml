@model wishlist.Models.Home
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="col">
        <h2>Wishlist</h2>
        <table>
            <tr>
                <th>name</th>
                <th>comment</th>
                <th>url</th>
                <th>added_on</th>
            </tr>
            @foreach (var row in Model.data)
            {
        <tr>
            <td>@row[0]</td>
            <td>@row[2]</td>
            <td><a href="@row[1]" target="_blank">Link</a></td>
            <td>@row[3]</td>
        </tr>
            }
        </table>
    </div>
</div>
